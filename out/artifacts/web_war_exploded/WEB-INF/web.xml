<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>receive</servlet-name>
        <servlet-class>Course.CourseServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>receive</servlet-name>
        <url-pattern>/course</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>community</servlet-name>
        <servlet-class>Community.CommunityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>community</servlet-name>
        <url-pattern>/community</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>create</servlet-name>
        <servlet-class>Handle.Create</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>create</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
</web-app>
